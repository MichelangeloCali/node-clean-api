{
  "name": "express-mongo",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/moxios": "^0.4.15",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "moxios": "^0.4.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "6.2.6"
  },
  "scripts": {
    "precommit": "tsc --noEmit && lint-staged",
    "dev:server": "nodemon",
    "dev:server-debug": "node -r ts-node/register --inspect=5858 src/index.ts",
    "lint": "tsc --noEmit && eslint '{src,__test__}/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:fix": "eslint . --fix",
    "obsolete:prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "node .",
    "test": "jest",
    "dev:tdd": "node --inspect=5859 node_modules/.bin/jest --runInBand --watch",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --quiet",
      "git add"
    ]
  }
}
